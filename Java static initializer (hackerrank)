import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
public static int  B, H; // declare static variables
public static boolean flag = false; //set boolean flag to false 
//Write your code here

static{
    Scanner sc = new Scanner(System.in);
    B = sc.nextInt();
    H = sc.nextInt();
    
    if(B >0 && H>0){ //if B and H is greater than  return true
        flag = true;
    }
    else{
        System.out.println("java.lang.Exception: Breadth and height must be positive "); //return false if B& H <0
    }
}

public static void main(String[] args){
		if(flag){
			int area=B*H;
			System.out.print(area);
		}
		
	}//end of main

}//end of class
